{
  "name": "@momentum-design/momentum-abstract-builder",
  "packageManager": "yarn@3.2.4",
  "files": [
    "./dist/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "publish": "yarn npm publish",
    "test:prebuild": "yarn build:module",
    "analyze:prebuild": "yarn build:module",
    "analyze": "yarn analyze:lint && yarn analyze:syntax",
    "analyze:lint": "eslint \"./src/**\"",
    "analyze:syntax": "tsc --noEmit",
    "analyze:postbuild": "echo 'no analyze postbuild'",
    "test:postbuild": "echo 'no test postbuild'",
    "init-build": "yarn && yarn clean && yarn build",
    "build": "yarn build:module",
    "sync": "node dist/sync",
    "commit": "node dist/commit",
    "build:module": "tsc",
    "clean": "yarn clean:dist",
    "clean:dist": "rimraf ./dist/"
  },
  "dependencies": {
    "momentum-abstract": "^1.0.34",
    "momentum-constructor-common": "^1.1.5"
  },
  "devDependencies": {
    "jest": "^29.2.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
