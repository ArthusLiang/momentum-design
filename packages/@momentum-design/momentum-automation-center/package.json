{
	"name": "@momentum-design/momentum-automation-center",
	"version": "1.0.0",
	"packageManager": "yarn@3.2.4",
	"private": true,
	"files": [
		"./dist/*"
	],
	"description": "store momentum tokens icons and send out to other repos",
	"main": "src/index.js",
	"scripts": {
		"test": "jest",
		"build": "rm -rf ./dist/ && rm -rf ./output/ && tsc",
		"init-build": "yarn && yarn build",
		"gen-color": "node dist/tools/generator-color",
		"gen-icon": "node dist/tools/generator-icon",
		"gen-illustration": "node dist/tools/generator-illustration",
		"gen-font": "node dist/tools/generator-font",
		"gen-version": "node dist/tools/version",
		"publish-abstract": "node dist/tools/publish",
		"commit": "node dist/tools/commit",
		"pack": "cd release/momentum-abstract && npm pack",
		"analyze": "yarn analyze:prebuild && yarn analyze:postbuild",
		"analyze:postbuild": "echo \"script 'analyze:postbuild' has not been implemented\"",
		"analyze:prebuild": "echo \"script 'analyze:prebuild' has not been implemented\"",
		"clean": "echo \"script 'clean' has not been implemented\"",
		"docs": "echo \"script 'docs' has not been implemented\"",
		"publish": "yarn publish:npmjs",
		"publish:npmjs": "echo \"script 'publish:npmjs' has not been implemented\"",
		"test:postbuild": "jest",
		"test:prebuild": "yarn init-build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/momentum-design/momentum-automation-center.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/momentum-design/momentum-automation-center/issues"
	},
	"homepage": "https://github.com/momentum-design/momentum-automation-center#readme",
	"devDependencies": {
		"@jsdevtools/npm-publish": "^1.4.3",
		"@octokit/core": "^3.5.1",
		"@types/node": "17.0.26",
		"cli-progress": "3.11.1",
		"color-convert": "^2.0.1",
		"jest": "28.1.3",
		"octokit-plugin-create-pull-request": "^3.11.0",
		"svgo": "^2.8.0",
		"tslint": "6.1.3",
		"typescript": "4.6.3",
		"write": "^2.0.0",
		"yargs-parser": "20.2.6"
	}
}
